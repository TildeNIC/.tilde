zone "master.tilde" {
	type master;
	file "/etc/bind/db.master.tilde";
};

zone "team.tilde" {
	type master;
	file "/etc/bind/db.team.tilde";
};

zone "your.tilde" IN {
    type forward; 
    forwarders { 149.56.184.112; };
};

zone "chat.tilde" {
	tpye master;
	file "/etc/bind/db.chat.tilde";
};
